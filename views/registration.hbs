<div class="container">
	<table class="table table-responsive table-hover table-striped">
    <div class="fh5co-narrow-content">
      <h1>Please Sign Up To get Start</h1>

    </div>
	<div class="row">
    <div class="col-md-8">
      <section>

        <hr>
        {{#if errors }}
        <ul class="alert alert-danger" >
        {{# each errors}}

<li>   {{this.msg}}</li>

        {{/each}}
      </ul>
      {{/if}}
            <form class="form-horizontal" method="post" action="/users/signup"  >
        <tr><td><div class="form-group">
          <label class="control-label col-sm-3">Email ID <span class="text-danger">*</span></label>
        <div class="col-md-8 col-sm-9">
              <div class="input-group">
              <span class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
              <input type="email" value="someone@example.com" required class="form-control"  name="email"   placeholder="Enter your Email ID">
            </div>

            <small> Your Email Id is being used for ensuring the security of your account, authorization and access recovery. </small> </div>
        </div></td></tr>

        <tr><td><div class="form-group">
          <label class="control-label col-sm-3">Set Password <span class="text-danger">*</span></label>
          <div class="col-md-5 col-sm-8">
            <div class="input-group">
              <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
              <input type="password" value="12345678" required class="form-control" name="password"  placeholder="Choose password (5-15 chars)" >
           </div>
          </div>
        </div></td></tr>
        <tr><td><div class="form-group">
          <label class="control-label col-sm-3">Confirm Password <span class="text-danger">*</span></label>
          <div class="col-md-5 col-sm-8">
            <div class="input-group">
              <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
              <input type="password" value="12345678" required class="form-control" name="cpassword"  placeholder="Confirm your password" >
            </div>
          </div>
        </div></td></tr>
        <tr><td><div class="form-group">
          <label class="control-label col-sm-3">Full Name <span class="text-danger">*</span></label>
          <div class="col-md-8 col-sm-9">
            <input type="text" class="form-control" required value="Saif Raza" name="username"  placeholder="Enter your Name here" >
          </div>
        </div></td></tr>


        <tr><td><div class="form-group">
          <label class="control-label col-sm-3">Contact No. <span class="text-danger">*</span></label>
          <div class="col-md-5 col-sm-8">
          	<div class="input-group">
              <span class="input-group-addon"><i class="glyphicon glyphicon-phone"></i></span>
            <input type="text" class="form-control" required value="03087256653" name="contact"  placeholder="Enter your Primary contact no." >
            </div>
          </div>
        </div></td></tr>

         <tr><td><div class="form-group">
          <label class="control-label col-sm-3">Address <br>
          <small>(if any)</small></label>
          <div class="col-md-5 col-sm-8">
            <input type="text" class="form-control" required name="address" id="contactnum2" placeholder="Any other or Landline no (if any)" value="karachi">
          </div>
        </div></td></tr>
        <tr><td><div class="form-group">
          <label class="control-label col-sm-3">City <br>
          <small>(if any)</small></label>
          <div class="col-md-5 col-sm-8">
            <input type="text" class="form-control" required name="city"  placeholder="Any other or Landline no (if any)" value="karachi">
          </div>
        </div></td></tr>
        <tr><td><div class="form-group">
          <label class="control-label col-sm-3">Country <br>
          <small>(if any)</small></label>
          <div class="col-md-5 col-sm-8">
            <input type="text" class="form-control" required name="country"  placeholder="Any other or Landline no (if any)" value="pakistan">
          </div>
        </div></td></tr>


      <tr><td>  <div class="form-group">
          <div class="col-xs-offset-3 col-md-8 col-sm-9"><span class="text-muted"><span class="label label-danger">Note:-</span> By clicking Sign Up, you agree to our <a href="#">Terms</a> and that you have read our <a href="#">Policy</a>, including our <a href="#">Cookie Use</a>.</span> </div>
        </div>
        <tr><td><div class="form-group">
          <div class="col-xs-offset-3 col-xs-10">
            <input name="Submit" type="submit" value="Sign Up" class="btn btn-primary">
          </div>
        </div><td></tr>
      </form>


    </div>
</div>
</div>




</table>

